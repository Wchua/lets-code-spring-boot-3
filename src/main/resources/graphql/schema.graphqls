type Query {

 customers : [Customer]
 customersByName(name:String) : [Customer]
 customerById( id :ID ) :  Customer!
}

type Mutation  {
 createCustomer(name:String, subscribed: Boolean) : Customer
}

type Subscription {
 newCustomers: Customer
}

type Customer {
 id: ID
 name: String
 profile: Profile
}

type Profile {
 id: ID
}
